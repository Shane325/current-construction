<% projects.forEach(function(project){ %>
    <article class="portfolio-item <%=project.categories %>">
        <div class="portfolio-image">
            <a href="/project/<%=project.id %>">
        <img src="<%=project.portfolioImg.src %>" alt="<%=project.portfolioImg.alt %>">
								</a>
            <div class="portfolio-overlay">
                <a href="/project/<%=project.id %>" class="right-icon"><i class="icon-line-ellipsis"></i></a>
            </div>
        </div>
        <div class="portfolio-desc">
            <h3>
                <a href="/project/<%=project.id %>">
                    <%=project.name %>
                </a>
            </h3>
            <span><a href="/project/<%=project.id %>"><%=project.subtext %></a></span>
        </div>
    </article>
    <% }) %>
